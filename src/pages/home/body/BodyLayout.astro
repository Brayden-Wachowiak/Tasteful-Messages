---
import { Card } from '../../../components/index';
---

<div class="body-container">
    <Card 
        name="asd"
        price={12}
        image="asd"
        color="#fab855"
    />
</div>

<style>
    .body-container{
        grid-row: 3 / span 1;
        height: calc(100% -50px);
        margin-bottom: 50px;
        background-color: #fef9e6;
        border-radius: 15px;
        box-shadow: 0 3px 5px rgba(0, 0, 0, 0.3);
    }
</style>